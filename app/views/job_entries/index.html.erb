<p id="notice"><%= notice %></p>

<h1>Job Entries</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Job</th>
      <th>From</th>
      <th>Until</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_entries.each do |job_entry| %>
      <tr>
        <td><%= job_entry.user_id %></td>
        <td><%= job_entry.job_id %></td>
        <td><%= job_entry.from %></td>
        <td><%= job_entry.until %></td>
        <td><%= link_to 'Show', job_entry %></td>
        <td><%= link_to 'Edit', edit_job_entry_path(job_entry) %></td>
        <td><%= link_to 'Destroy', job_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Entry', new_job_entry_path %>
